{
    "name": "bitbybit",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:docs": "ng serve bitbybit-docs",
        "build": "ng build --prod --configuration=production",
        "build:docs": "ng build bitbybit-docs --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "scully": "scully --project bitbybit --prod",
